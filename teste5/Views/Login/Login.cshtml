
@{
    ViewBag.Title = "Login";
    <link rel="stylesheet" href="~/Content/Login.css" />

}

<div class="login_card">
    <div class="card_tudo">
        <h1>Faça o Login Para continuar</h1>

        <input type="email" class="email_Login" placeholder="Coloca o Email">
